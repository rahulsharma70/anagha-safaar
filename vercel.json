{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_SUPABASE_URL": "@vite_supabase_url",
    "VITE_SUPABASE_PUBLISHABLE_KEY": "@vite_supabase_publishable_key",
    "VITE_AMADEUS_API_KEY": "@vite_amadeus_api_key",
    "VITE_AMADEUS_API_SECRET": "@vite_amadeus_api_secret",
    "VITE_RAZORPAY_KEY_ID": "@vite_razorpay_key_id",
    "VITE_RAZORPAY_KEY_SECRET": "@vite_razorpay_key_secret",
    "VITE_OPENAI_API_KEY": "@vite_openai_api_key",
    "VITE_SENTRY_DSN": "@vite_sentry_dsn",
    "VITE_MIXPANEL_TOKEN": "@vite_mixpanel_token",
    "VITE_GOOGLE_ANALYTICS_ID": "@vite_google_analytics_id",
    "VITE_SENDGRID_API_KEY": "@vite_sendgrid_api_key",
    "VITE_TWILIO_ACCOUNT_SID": "@vite_twilio_account_sid",
    "VITE_TWILIO_AUTH_TOKEN": "@vite_twilio_auth_token",
    "VITE_TWILIO_PHONE_NUMBER": "@vite_twilio_phone_number"
  }
}
